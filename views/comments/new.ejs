<% include partials/header %>


<h1>Comment to <%=questions.fchoice%><h1>
<div class="container">
<div class="row" style="width:30%;margin:50px auto">
	<form action="/all/<%=questions._id%>/comments" method="post">
		<div class="form-group">
			<input type="text" placeholder="firstchoice" name="firstchoice" style="width:100%">
		</div>
		<div class="form-group">
			<input type="text" placeholder="secondchoice" name="secondchoice" style="width:100%">
		</div>
		<div class="form-group ">
			<button class="btn btn-lg btn-primary btn-block" > Submit</button>
		</div>
	</form>
</div>
</div>

<% include partials/footer %>